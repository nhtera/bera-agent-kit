{
  "name": "bera-agent-kit",
  "version": "1.1.0",
  "description": "Open-source agent kit for interacting with Berachain protocols",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Webera-Finance/bera-agent-kit.git"
  },
  "keywords": [
    "berachain",
    "agent",
    "blockchain",
    "defi",
    "automation"
  ],
  "author": "Webera Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Webera-Finance/bera-agent-kit/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/Webera-Finance/bera-agent-kit#readme",
  "scripts": {
    "clean": "rm -rf dist",
    "generate-dist-package-json": "node scripts/generate-dist-package.js",
    "build": "npm run clean && tsc && npm run generate-dist-package-json",
    "prepare": "npm run build",
    "test": "mocha -r ts-node/register 'test/**/*.spec.ts'",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "docs": "typedoc src --out docs",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "example": "ts-node examples/basic-usage.ts",
    "example:mcp": "ts-node examples/mcp-server.ts"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^1.0.0",
    "@ibm-cloud/watsonx-ai": "^1.3.0",
    "@langchain/community": "^0.3.22",
    "@langchain/core": "^0.3.27",
    "@langchain/langgraph": "^0.2.39",
    "@langchain/openai": "^0.3.17",
    "@modelcontextprotocol/sdk": "^1.6.1",
    "@playwright/test": "^1.42.1",
    "arg": "^5.0.2",
    "axios": "^1.7.9",
    "chai": "^4.3.7",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.7",
    "ibm-cloud-sdk-core": "^5.1.0",
    "langsmith": "^0.2.14",
    "mocha": "^11.0.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.73.1",
    "sinon": "^19.0.2",
    "typedoc": "^0.27.6",
    "viem": "^2.23.5",
    "winston": "^3.17.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
